{
    "version": 2,
    "builds": [
      {
        "src": "./api/index.js",
        "use": "@vercel/node",
        "config": {"includeFiles": ["lib/db.js", "lib/utils.js", "lib/cloudinary.js"]}
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/index.js"
      }
    ]
}